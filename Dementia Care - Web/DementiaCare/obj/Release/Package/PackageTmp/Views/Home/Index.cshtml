@{
    ViewBag.Title = "Dementia Care";
}

<div class="container-fluid no-padding">
    <div class="row no-margin" id="section1">
        <div class="col-12 no-padding full-height">
            <img src="~/Images/background.jpg" class="img-fit" alt="Dementia Care" />

            <div class="sticky-top abs-position full-width">
                <div class="container-fluid no-padding">
                    <div class="row no-margin">
                        <div class="col-lg-4 no-padding">
                            <img src="~/Images/title.png" class="img-center" alt="" />
                        </div>
                    </div>
                </div>
            </div>

            <div class="abs-position full-width question-position">
                <div class="container-fluid no-padding">
                    <div class="row no-margin">
                        <div class="col-lg-4 no-padding" style="background: rgba(255,255,255,0.75);">
                            <h4 class="text-dark text-break text-center calibri-font" style="padding-top: 12px; padding-bottom: 12px; margin: 0;">
                                Does your family member suffer from dementia?
                                <br />
                                OR
                                <br />
                                Do you have trouble looking after them?
                            </h4>
                        </div>
                    </div>
                </div>
            </div>

            <div class="abs-position full-width" style="bottom: 10px;">
                <a href="#section2" class="img-anchor">
                    <img src="~/Images/downArrow.png" class="img-down-arrow" alt="" />
                </a>
            </div>
        </div>
    </div>

